

    <div class="form-group">
        <label for="title">Title</label>
        <input type="text"
               value="<%= book.title %>"
               name="title"
               class="form-control"
               id="title">
    </div>

    <div class="form-group">
        <label for="description">Description</label>
        <input type="text"
               value="<%= book.description %>"
               name="description"
               class="form-control"
               id="description">
    </div>

    <div class="form-group">
        <label for="authors">Authors</label>
        <input type="text"
               value="<%= book.authors %>"
               name="authors"
               class="form-control"
               id="authors">
    </div>

    <div class="form-group">
        <label for="favorite">Favorite</label>
        <input type="text"
               value="<%= book.favorite %>"
               name="favorite"
               class="form-control"
               id="favorite">
    </div>

    <div class="form-group">
        <label for="fileCover">FileCover</label>
        <input type="text"
               value="<%= book.fileCover %>"
               name="fileCover"
               class="form-control"
               id="fileCover">
    </div>

